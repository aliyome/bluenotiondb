{
  "name": "bluenotiondb",
  "module": "src/index.ts",
  "type": "module",
  "version": "0.4.2",
  "scripts": {
    "main": "DEBUG=1 bun run src/index.ts",
    "dry-run": "BLUE_NOTION_DRY_RUN=true bun run src/index.ts",
    "dist": "bun build src/index.ts --compile --outfile bluenotiondb",
    "test": "eslint \"**/*.ts\"",
